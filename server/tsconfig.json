{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": [
      "es2020"
    ],
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./"
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "*.js",
    "dist",
    "test_*.ts",
    "setup_*.js",
    "change_*.js",
    "cleanup_*.js"
  ]
}